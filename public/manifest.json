{
  "manifest_version": 3,
  "name": "Habit Tracker",
  "version": "1.0.0",
  "description": "Track and manage your daily habits",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"

  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}